    color: #aaa;
}

.time-left {
    float: left;
    color: #999;
}
.caja_mensaje{
    display:block ;
    position: fixed;
    border: 1px solid black;
    bottom:50px;    

}
.caja_mensaje input{
    height: 50px;
    background: white;
    border-radius: 10px;
}
.caja_mensaje button{
    height: 50px;
    border-radius: 10px;
}

</style>

